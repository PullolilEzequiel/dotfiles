{
  "custom/mediaplayer": {
    "exec": "playerctl metadata --format '{{title}}'",
    "return-type": "text",
    "interval": 5,
    "format": " {}",
    "on-click": "playerctl play-pause",
    "on-scroll-up": "playerctl next",
    "on-scroll-down": "playerctl previous",
    "max-length": 15,
  },
}
